<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:e="clr-namespace:BinaryStudio.Security.Cryptography.PlatformUI.Controls"
                    xmlns:u="http://schemas.helix.global">
  <DataTemplate DataType="{x:Type e:Asn1GridEntry}" x:Key="Asn1GridEntryStyle.TextBox">
    <TextBox Text="{Binding Value, Mode=OneWay}"
             BorderBrush="{x:Null}"
             BorderThickness="0"
             Background="Transparent"
             Foreground="{DynamicResource ControlTextBrushKey}"
             CaretBrush="{DynamicResource ControlTextBrushKey}"
             IsReadOnly="True"
             IsReadOnlyCaretVisible="True"
             Focusable="True"
             Margin="-3,-1,-1,0"/>
  </DataTemplate>
  <Style x:Key="ExpandOrCollapseToggleButtonStyle" TargetType="{x:Type ToggleButton}">
    <Setter Property="UseLayoutRounding" Value="True"/>
    <Setter Property="ClickMode" Value="Press"/>
    <Setter Property="Background" Value="{DynamicResource ControlBrushKey}"/>
    <Setter Property="SnapsToDevicePixels" Value="True"/>
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type ToggleButton}">
          <Grid Margin="5,0,0,0" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
            <Rectangle Fill="Transparent"/>
            <Path Name="Collapsed"
                  HorizontalAlignment="Center"
                  VerticalAlignment="Center"
                  StrokeThickness="1"
                  Stroke="{DynamicResource ControlTextBrushKey}"
                  Fill="{TemplateBinding Background}"
                  Data="M 0.5 0.5 L 8.5 0.5 L 8.5 8.5 L 0.5 8.5 L 0.5 0.5 M 2.5 4.5 L 6.5 4.5 M 4.5 2.5 L 4.5 6.5">
            </Path>
            <Path Name="Expanded"
                  HorizontalAlignment="Center"
                  VerticalAlignment="Center"
                  Margin="1,1,1,1"
                  StrokeThickness="1"
                  Stroke="{DynamicResource ControlTextBrushKey}"
                  Data="M 0.5 0.5 L 8.5 0.5 L 8.5 8.5 L 0.5 8.5 L 0.5 0.5 M 2.5 4.5 L 6.5 4.5"
                  Fill="{TemplateBinding Background}"
                  Visibility="Hidden">
            </Path>
          </Grid>
          <ControlTemplate.Triggers>
            <Trigger Property="IsChecked" Value="True">
              <Setter Property="Visibility" Value="Visible"   TargetName="Expanded"/>
              <Setter Property="Visibility" Value="Collapsed" TargetName="Collapsed"/>
            </Trigger>
            <Trigger Property="IsChecked" Value="False">
              <Setter Property="Visibility" Value="Collapsed" TargetName="Expanded"/>
              <Setter Property="Visibility" Value="Visible"   TargetName="Collapsed"/>
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style TargetType="{x:Type e:Asn1Control}">
    <Setter Property="BorderBrush" Value="Red"/>
    <Setter Property="BorderThickness" Value="0"/>
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type e:Asn1Control}">
          <Border Background="{TemplateBinding Background}"
                  BorderBrush="{TemplateBinding BorderBrush}"
                  BorderThickness="{TemplateBinding BorderThickness}">
            <Border.Resources>
              <ContextMenu x:Key="ContextMenu">
                <MenuItem Header="Save..." Command="Save" CommandParameter="{Binding}"/>
                <MenuItem Header="Copy (BASE64)" Command="{x:Static e:Asn1Control.CopyBase64}" CommandParameter="{Binding}"/>
                <MenuItem Header="Copy (Json)"   Command="{x:Static e:Asn1Control.CopyJson}"   CommandParameter="{Binding}"/>
              </ContextMenu>
              <HierarchicalDataTemplate DataType="{x:Type e:Asn1GridEntry}" ItemsSource="{Binding Entries}"/>
              <Style TargetType="{x:Type Border}">
                <Setter Property="BorderBrush" Value="{DynamicResource ControlDarkBrushKey}"/>
                <Setter Property="Background" Value="{DynamicResource ControlDarkBrushKey}"/>
                <Setter Property="BorderThickness" Value="1,0,0,1"/>
              </Style>
              <Style TargetType="{x:Type TextBlock}">
                <Setter Property="Margin" Value="5,0,5,0"/>
                <Setter Property="TextTrimming" Value="CharacterEllipsis"/>
              </Style>
              <Style TargetType="{x:Type GridSplitter}">
                <Setter Property="ShowsPreview" Value="False"/>
                <Setter Property="Background" Value="Transparent"/>
                <Setter Property="Focusable" Value="False"/>
                <Setter Property="HorizontalAlignment" Value="Right"/>
                <Setter Property="ResizeDirection" Value="Columns"/>
                <Setter Property="Width" Value="5"/>
              </Style>
            </Border.Resources>
            <Grid>
              <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition/>
              </Grid.RowDefinitions>
              <StackPanel Orientation="Horizontal" Margin="2">
                <ToggleButton Style="{StaticResource {x:Static ToolBar.ToggleButtonStyleKey}}"
                              IsChecked="{Binding RelativeSource={RelativeSource TemplatedParent},Path=DecodeObjectIdentifier,Mode=TwoWay}">
                  <ToggleButton.ToolTip>
                    <ToolTip>
                      <!--<TextBlock Text="{u:LocalResource Source={x:Static r:Resources.ResourceManager},Culture={Binding RelativeSource={RelativeSource TemplatedParent},Path=(u:LocalizationManager.Culture)},Key=DecodeObjectIdentifier}"/>-->
                    </ToolTip>
                  </ToggleButton.ToolTip>
                  <Viewbox Width="16" Height="16">
                    <Rectangle Width="16" Height="16">
                      <Rectangle.Fill>
                        <DrawingBrush>
                          <DrawingBrush.Drawing>
                            <DrawingGroup>
                              <DrawingGroup.Children>
                                <GeometryDrawing Brush="#00FFFFFF" Geometry="F1M16,16L0,16 0,0 16,0z" />
                                <GeometryDrawing Brush="{DynamicResource ControlBrushKey}"     Geometry="F1M11.5,0C9.186,0,7.302,1.753,7.051,4L1.585,4 0,5.586 0,12.414 1.586,14 2,14 2,16 4.414,16 6.414,14 9.414,14 11,12.414 11,8.952C11.165,8.973 11.329,9 11.5,9 13.985,9 16,6.985 16,4.5 16,2.015 13.985,0 11.5,0" />
                                <GeometryDrawing Brush="{DynamicResource ControlTextBrushKey}" Geometry="F1M9,12L5.5,12 4,13.5 4,12 2,12 2,6 7.275,6C7.162,5.681,7.089,5.347,7.051,5L1.999,5 1,6 1,12 2,13 3,13 3,15 4,15 6,13 9,13 10,12 10,8.725C9.646,8.599,9.307,8.445,9,8.24z" />
                                <GeometryDrawing Brush="{DynamicResource ControlBrushKey}"     Geometry="F1M2,12L4,12 4,13.5 5.5,12 9,12 9,8.24C8.203,7.707,7.603,6.919,7.275,6L2,6z" />
                                <GeometryDrawing Brush="{DynamicResource DesktopBrushKey}"     Geometry="F1M12,3L11,3 11,2 12,2z M12,7L11,7 11,4 12,4z M11.5,1C9.567,1 8,2.567 8,4.5 8,6.433 9.567,8 11.5,8 13.433,8 15,6.433 15,4.5 15,2.567 13.433,1 11.5,1" />
                                <GeometryDrawing Brush="{DynamicResource ControlBrushKey}"     Geometry="F1M11,3L12,3 12,1.999 11,1.999z M11,7L12,7 12,4 11,4z" />
                              </DrawingGroup.Children>
                            </DrawingGroup>
                          </DrawingBrush.Drawing>
                        </DrawingBrush>
                      </Rectangle.Fill>
                    </Rectangle>
                  </Viewbox>
                </ToggleButton>
                <Button Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}" Command="{x:Static ApplicationCommands.SaveAs}" CommandParameter="{Binding}">
                  <Viewbox Width="16" Height="16">
                    <Rectangle Width="16" Height="16">
                      <Rectangle.Fill>
                        <DrawingBrush>
                          <DrawingBrush.Drawing>
                            <DrawingGroup>
                              <DrawingGroup.Children>
                                <GeometryDrawing Brush="#00FFFFFF" Geometry="F1M16,16L0,16 0,0 16,0z"/>
                                <GeometryDrawing Brush="#FFF6F6F6" Geometry="F1M15,15L1,15 1,1 13,1 15,3z" />
                                <GeometryDrawing Brush="#FF00539C" Geometry="F1M10,2L8,2 8,5 10,5z M14,4L14,14 2,14 2,2 5,2 5,7 11,7 11,2 12,2z" />
                                <GeometryDrawing Brush="#FFF0EFF1" Geometry="F1M11,2L11,7 5,7 5,2 8,2 8,5 10,5 10,2z" />
                              </DrawingGroup.Children>
                            </DrawingGroup>
                          </DrawingBrush.Drawing>
                        </DrawingBrush>
                      </Rectangle.Fill>
                    </Rectangle>
                  </Viewbox>
                </Button>
                <Button Command="Zoom">Copy...</Button>
              </StackPanel>
              <Grid Grid.Row="1" Name="HostGrid">
                <Grid.ColumnDefinitions>
                  <ColumnDefinition Width="{Binding RelativeSource={RelativeSource AncestorType={x:Type e:Asn1Control}},Path=Columns[0].Width,Mode=TwoWay}"/>
                  <ColumnDefinition Width="{Binding RelativeSource={RelativeSource AncestorType={x:Type e:Asn1Control}},Path=Columns[1].Width,Mode=TwoWay}"/>
                  <ColumnDefinition Width="{Binding RelativeSource={RelativeSource AncestorType={x:Type e:Asn1Control}},Path=Columns[2].Width,Mode=TwoWay}"/>
                  <ColumnDefinition Width="{Binding RelativeSource={RelativeSource AncestorType={x:Type e:Asn1Control}},Path=Columns[3].Width,Mode=TwoWay}"/>
                  <ColumnDefinition Width="{Binding RelativeSource={RelativeSource AncestorType={x:Type e:Asn1Control}},Path=Columns[4].Width,Mode=TwoWay}"/>
                  <ColumnDefinition/>
                  <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                  <RowDefinition Height="Auto"/>
                  <RowDefinition/>
                  <RowDefinition/>
                </Grid.RowDefinitions>
                <Border Grid.Row="0" Grid.Column="0">
                  <TextBlock Text="Node"
                             TextAlignment="Center"/>
                </Border>
                <Border Grid.Row="0" Grid.Column="1">
                  <TextBlock Text="Class"/>
                </Border>
                <Border Grid.Row="0" Grid.Column="2">
                  <TextBlock Text="Type"/>
                </Border>
                <Border Grid.Row="0" Grid.Column="3">
                  <TextBlock Text="Offset"/>
                </Border>
                <Border Grid.Row="0" Grid.Column="4">
                  <TextBlock Text="Size"/>
                </Border>
                <Border Grid.Row="0" Grid.Column="5">
                  <TextBlock Text="Value"/>
                </Border>
                <Border Grid.Column="0" Grid.ColumnSpan="7" Grid.Row="1" Background="Transparent" BorderThickness="1,0,1,1">
                  <ScrollViewer VerticalScrollBarVisibility="Auto" CanContentScroll="True" Background="Transparent" Margin="-1,0,-1,0">
                    <TreeView Name="ItemsHost">
                      <TreeView.ItemsPanel>
                        <ItemsPanelTemplate>
                          <VirtualizingStackPanel CanHorizontallyScroll="False" IsVirtualizing="True" VirtualizationMode="Recycling"/>
                        </ItemsPanelTemplate>
                      </TreeView.ItemsPanel>
                      <TreeView.Template>
                        <ControlTemplate TargetType="{x:Type TreeView}">
                          <Grid>
                            <ItemsPresenter/>
                          </Grid>
                        </ControlTemplate>
                      </TreeView.Template>
                      <TreeView.ItemContainerStyle>
                        <Style TargetType="{x:Type TreeViewItem}">
                          <Setter Property="Foreground" Value="{DynamicResource ControlTextBrushKey}"/>
                          <Setter Property="IsExpanded" Value="{Binding IsExpanded,Mode=TwoWay,IsAsync=True}"/>
                          <Setter Property="IsSelected" Value="{Binding IsSelected,Mode=TwoWay,IsAsync=True}"/>
                          <Setter Property="Template">
                            <Setter.Value>
                              <ControlTemplate TargetType="{x:Type TreeViewItem}">
                                <Grid>
                                  <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="{Binding Owner.Columns[0].Width,Mode=OneWay}"/>
                                    <ColumnDefinition Width="{Binding Owner.Columns[1].Width,Mode=OneWay}"/>
                                    <ColumnDefinition Width="{Binding Owner.Columns[2].Width,Mode=OneWay}"/>
                                    <ColumnDefinition Width="{Binding Owner.Columns[3].Width,Mode=OneWay}"/>
                                    <ColumnDefinition Width="{Binding Owner.Columns[4].Width,Mode=OneWay}"/>
                                    <ColumnDefinition/>
                                  </Grid.ColumnDefinitions>
                                  <Grid.Resources>
                                    <Style TargetType="{x:Type TextBlock}">
                                      <Setter Property="Margin" Value="5,0,5,0"/>
                                      <Setter Property="TextTrimming" Value="CharacterEllipsis"/>
                                    </Style>
                                  </Grid.Resources>
                                  <Grid.RowDefinitions>
                                    <RowDefinition/>
                                    <RowDefinition/>
                                  </Grid.RowDefinitions>
                                  <Border Grid.Row="0" Grid.Column="0" Name="InnerBorder" BorderThickness="0,0,0,1" BorderBrush="{DynamicResource ControlDarkBrushKey}" Grid.ColumnSpan="5" Background="Transparent"/>
                                  <Border Grid.Row="0" Grid.Column="0" Name="OuterBorder"
                                          BorderThickness="1,0,0,0"
                                          TextBlock.Foreground="{TemplateBinding Foreground}"
                                          BorderBrush="{DynamicResource ControlDarkBrushKey}"
                                          Background="Transparent">
                                    <Grid>
                                      <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition/>
                                      </Grid.ColumnDefinitions>
                                      <ToggleButton Grid.Column="0"
                                            Name="Expander"
                                            Width="20"
                                            Margin="{Binding Margin}"
                                            IsChecked="{Binding IsExpanded, RelativeSource={RelativeSource TemplatedParent}}"
                                            Style="{StaticResource ExpandOrCollapseToggleButtonStyle}"/>
                                      <Grid Grid.Column="1">
                                        <Grid.ColumnDefinitions>
                                          <ColumnDefinition Width="Auto"/>
                                          <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <Canvas Grid.Column="0" Width="16" Height="16" VerticalAlignment="Top">
                                          <Viewbox>
                                            <Rectangle Width="16" Height="16" x:Name="TypeMarker">
                                              <Rectangle.Fill>
                                                <DrawingBrush>
                                                  <DrawingBrush.Drawing>
                                                    <DrawingGroup>
                                                      <DrawingGroup.Children>
                                                        <GeometryDrawing Brush="{DynamicResource ControlBrushKey}" Geometry="F1M6.6969,-0.000199999999999534L-9.99999999997669E-05,4.4648 -9.99999999997669E-05,10.5348 5.4999,14.2008 6.4999,13.5348 6.4999,14.5348 8.6979,15.9998 9.3019,15.9998 15.9999,11.5348 15.9999,5.4648 13.4999,3.7978 12.4999,4.4648 12.4999,3.4648 7.3029,-0.000199999999999534z" />
                                                        <GeometryDrawing Brush="{DynamicResource ControlTextBrushKey}" Geometry="F1M10.5,6.9995L7.5,8.9995 7.5,10.4645 6,11.4645 6,8.2675 10.5,5.2675z M2.353,5.3005L7,2.2005 10.147,4.3005 5.5,7.3975z M5,11.4645L2,9.4645 2,6.2675 5,8.2675z M13.5,4.9995L11.5,6.3325 11.5,3.9995 7,0.999499999999999 1,4.9995 1,9.9995 5.5,12.9995 7.5,11.6665 7.5,13.9995 9,14.9995 15,10.9995 15,5.9995z" />
                                                        <GeometryDrawing Brush="{DynamicResource ControlBrushKey}" Geometry="F1M7,2.2007L2.353,5.3007 5.5,7.3977 10.147,4.3007z" />
                                                        <GeometryDrawing Brush="{DynamicResource ControlBrushKey}" Geometry="F1M2,9.4648L5,11.4648 5,8.2668 2,6.2668z" />
                                                        <GeometryDrawing Brush="{DynamicResource ControlBrushKey}" Geometry="F1M6,8.2671L6,11.4651 7.5,10.4651 7.5,8.9991 10.5,6.9991 10.5,5.2671z" />
                                                      </DrawingGroup.Children>
                                                    </DrawingGroup>
                                                  </DrawingBrush.Drawing>
                                                </DrawingBrush>
                                              </Rectangle.Fill>
                                            </Rectangle>
                                          </Viewbox>
                                          <Rectangle Width="5" Height="5" Canvas.Left="0" Canvas.Bottom="0" x:Name="PictureLeftBottom" Visibility="Hidden"/>
                                          <Rectangle Width="5" Height="5" Canvas.Left="0" Canvas.Top="0"    x:Name="PictureLeftTop"    Visibility="Hidden"/>
                                        </Canvas>
                                        <TextBlock Text="{Binding Header,IsAsync=True}"
                                            Grid.Column="1"
                                            Margin="5,0,0,0"
                                            TextTrimming="CharacterEllipsis"/>
                                      </Grid>
                                    </Grid>
                                  </Border>
                                  <Border Grid.Row="0" Grid.Column="1" Name="ClassBorder"
                                        BorderBrush="{DynamicResource ControlDarkBrushKey}"
                                        Background="Transparent"
                                        BorderThickness="1,0,0,0">
                                    <TextBlock Text="{Binding Class}" TextTrimming="CharacterEllipsis"/>
                                  </Border>
                                  <Border Grid.Row="0" Grid.Column="2" Name="TypeBorder"
                                        BorderBrush="{DynamicResource ControlDarkBrushKey}"
                                        Background="Transparent"
                                        BorderThickness="1,0,0,0">
                                    <TextBlock Text="{Binding Type}" TextTrimming="CharacterEllipsis"/>
                                  </Border>
                                  <Border Grid.Row="0" Grid.Column="3" Name="OffsetBorder"
                                        BorderBrush="{DynamicResource ControlDarkBrushKey}"
                                        Background="Transparent"
                                        BorderThickness="1,0,0,0">
                                    <TextBlock Text="{Binding Offset}" TextTrimming="CharacterEllipsis"/>
                                  </Border>
                                  <Border Grid.Row="0" Grid.Column="4" Name="SizeBorder"
                                        BorderBrush="{DynamicResource ControlDarkBrushKey}"
                                        Background="Transparent"
                                        BorderThickness="1,0,0,0">
                                    <TextBlock Text="{Binding Length}" TextTrimming="CharacterEllipsis"/>
                                  </Border>
                                  <Border Grid.Row="0" Grid.Column="5"
                                        BorderBrush="{DynamicResource ControlDarkBrushKey}"
                                        Background="Transparent"
                                        BorderThickness="1,0,0,1">
                                    <Grid>
                                      <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition/>
                                      </Grid.ColumnDefinitions>
                                      <Grid Grid.Column="0" Visibility="Hidden" Name="DecodingMarker" Width="8" Margin="2,0,0,0">
                                        <Rectangle Fill="Transparent"/>
                                        <Rectangle Fill="Orange" Width="4" Height="4" HorizontalAlignment="Center"/>
                                      </Grid>
                                      <e:Asn1GridEntryContentPresenter Grid.Column="1" x:Name="ValueHolder"
                                                                       EditTemplate="{StaticResource Asn1GridEntryStyle.TextBox}"
                                                                       Margin="5,0,5,0">
                                        <e:Asn1GridEntryContentPresenter.DisplayTemplate>
                                          <DataTemplate DataType="{x:Type e:Asn1GridEntry}">
                                            <Grid>
                                              <Rectangle Fill="Transparent"/>
                                              <TextBox Text="{Binding Value,Mode=OneWay}"
                                                       BorderBrush="{x:Null}"
                                                       BorderThickness="0"
                                                       Background="Transparent"
                                                       Foreground="{DynamicResource ControlTextBrushKey}"
                                                       CaretBrush="{DynamicResource ControlTextBrushKey}"
                                                       Opacity="0.5"
                                                       IsHitTestVisible="False"
                                                       IsReadOnly="True"
                                                       IsReadOnlyCaretVisible="True"
                                                       Margin="-3,-1,-1,0"/>
                                            </Grid>
                                          </DataTemplate>
                                        </e:Asn1GridEntryContentPresenter.DisplayTemplate>
                                      </e:Asn1GridEntryContentPresenter>
                                      <Viewbox Grid.Column="1" Name="EndOfContentPicture" Visibility="Collapsed" Width="8" Height="8" HorizontalAlignment="Left" Margin="4,0,0,0">
                                        <Ellipse Width="8" Height="8" Stroke="{DynamicResource ControlTextBrushKey}" StrokeThickness="1"/>
                                      </Viewbox>
                                    </Grid>
                                  </Border>
                                  <ItemsPresenter Grid.Column="0" Grid.ColumnSpan="6" Grid.Row="1"
                                                Name="ItemsHost"
                                                Visibility="Collapsed"
                                                SnapsToDevicePixels="True"/>
                                </Grid>
                                <ControlTemplate.Triggers>
                                  <Trigger Property="HasItems" Value="False">
                                    <Setter TargetName="Expander" Property="Visibility" Value="Hidden" />
                                  </Trigger>
                                  <Trigger Property="IsExpanded" Value="True">
                                    <Setter TargetName="ItemsHost" Property="Visibility" Value="Visible"/>
                                  </Trigger>
                                  <MultiTrigger>
                                    <MultiTrigger.Conditions>
                                      <Condition Property="IsSelected" Value="True"/>
                                    </MultiTrigger.Conditions>
                                    <Setter TargetName="InnerBorder" Property="Background" Value="{DynamicResource HighlightBrushKey}"/>
                                    <Setter TargetName="OuterBorder" Property="TextBlock.Foreground" Value="{DynamicResource HighlightTextBrushKey}"/>
                                    <!--<Setter TargetName="SizeBorder" Property="Background" Value="{DynamicResource HighlightBrushKey}"/>-->
                                  </MultiTrigger>
                                  <DataTrigger Binding="{Binding Type,Mode=OneWay}" Value="Sequence">
                                    <Setter TargetName="TypeMarker" Property="Fill">
                                      <Setter.Value>
                                        <DrawingBrush>
                                          <DrawingBrush.Drawing>
                                            <DrawingGroup>
                                              <DrawingGroup.Children>
                                                <GeometryDrawing Brush="#00FFFFFF" Geometry="F1M16,16L0,16 0,0 16,0z" />
                                                <GeometryDrawing Brush="{DynamicResource ControlBrushKey}"        Geometry="F1M6.0003,-0.000199999999999534L6.0003,1.5858 5.4143,0.9998 3.9993,0.9998 3.9993,4.0008 1.0003,4.0008 1.0003,13.0008 3.9993,13.0008 3.9993,13.4148 6.5863,15.9998 9.4143,15.9998 11.9993,13.4148 11.9993,13.0008 15.0003,13.0008 15.0003,4.0008 11.9993,4.0008 11.9993,0.9998 10.5853,0.9998 10.0003,1.5858 10.0003,-0.000199999999999534z" />
                                                <GeometryDrawing Brush="{DynamicResource ControlTextBrushKey}"  Geometry="F1M7.0002,9.9997L7.0002,13.0007 5.0002,10.9997 5.0002,13.0007 8.0002,15.9997 11.0002,13.0007 11.0002,10.9997 9.0002,13.0007 9.0002,9.9997z" />
                                                <GeometryDrawing Brush="{DynamicResource ControlTextBrushKey}"  Geometry="F1M11,3.9995L11,1.9995 9,3.9995 9,0.999499999999999 7,0.999499999999999 7,3.9995 5,1.9995 5,3.9995 8,6.9995z" />
                                                <GeometryDrawing Brush="{DynamicResource ControlTextBrushKey}"  Geometry="F1M3,6L5.586,6 4.586,5 2,5 2,12 4,12 4,11 3,11z" />
                                                <GeometryDrawing Brush="{DynamicResource ControlTextBrushKey}"  Geometry="F1M11.4141,5L10.4141,6 13.0001,6 13.0001,11 12.0001,11 12.0001,12 14.0001,12 14.0001,5z" />
                                                <GeometryDrawing Brush="{DynamicResource ControlBrushKey}"        Geometry="F1M10.4141,6L8.4141,8 7.5861,8 5.5861,6 3.0001,6 3.0001,11 4.0001,11 4.0001,10 5.4141,10 6.0001,10.586 6.0001,9 10.0001,9 10.0001,10.586 10.5861,10 12.0001,10 12.0001,11 13.0001,11 13.0001,6z" />
                                              </DrawingGroup.Children>
                                            </DrawingGroup>
                                          </DrawingBrush.Drawing>
                                        </DrawingBrush>
                                      </Setter.Value>
                                    </Setter>
                                  </DataTrigger>
                                  <DataTrigger Binding="{Binding Type,Mode=OneWay}" Value="Set">
                                    <Setter TargetName="TypeMarker" Property="Fill">
                                      <Setter.Value>
                                        <DrawingBrush>
                                          <DrawingBrush.Drawing>
                                            <DrawingGroup>
                                              <DrawingGroup.Children>
                                                <GeometryDrawing Brush="#00FFFFFF" Geometry="F1M16,16L0,16 0,0 16,0z" />
                                                <GeometryDrawing Brush="{DynamicResource ControlBrushKey}"     Geometry="F1M9,-0.000199999999999534L9,4.0008 8,4.0008 6,4.0008 0,4.0008 0,12.0008 6,12.0008 8,12.0008 9,12.0008 9,15.9998 16,15.9998 16,8.9998 12.414,8.9998 13,8.4138 13,7.5858 12.414,6.9998 16,6.9998 16,-0.000199999999999534z" />
                                                <GeometryDrawing Brush="{DynamicResource ControlTextBrushKey}" Geometry="F1M15,1L10,1 10,6 15,6z" />
                                                <GeometryDrawing Brush="{DynamicResource ControlTextBrushKey}" Geometry="F1M15,10L10,10 10,15 15,15z" />
                                                <GeometryDrawing Brush="{DynamicResource ControlTextBrushKey}" Geometry="F1M5,6L6.586,6 6,5.414 6,5 1,5 1,11 6,11 6,10.586 6.586,10 5,10z" />
                                                <GeometryDrawing Brush="{DynamicResource ControlTextBrushKey}" Geometry="F1M7,5L9,7 6,7 6,9 9,9 7,11 9,11 10,10 12,8 10,6 9,5z" />
                                              </DrawingGroup.Children>
                                            </DrawingGroup>
                                          </DrawingBrush.Drawing>
                                        </DrawingBrush>
                                      </Setter.Value>
                                    </Setter>
                                  </DataTrigger>
                                  <DataTrigger Binding="{Binding Type,Mode=OneWay}" Value="ObjectIdentifier">
                                    <Setter TargetName="TypeMarker" Property="Fill">
                                      <Setter.Value>
                                        <DrawingBrush>
                                          <DrawingBrush.Drawing>
                                            <DrawingGroup>
                                              <DrawingGroup.Children>
                                                <GeometryDrawing Brush="#00FFFFFF" Geometry="F1M16,16L0,16 0,0 16,0z" />
                                                <GeometryDrawing Brush="{DynamicResource ControlBrushKey}"     Geometry="F1M11,4C9.525,4,8.25,4.81,7.557,6L1,6 1,10 2,10 2,12 7,12 7,10 7.557,10C8.25,11.189 9.525,12 11,12 13.206,12 15,10.206 15,8 15,5.794 13.206,4 11,4" />
                                                <GeometryDrawing Brush="{DynamicResource ControlTextBrushKey}" Geometry="F1M11,9C10.449,9 10,8.551 10,8 10,7.449 10.449,7 11,7 11.553,7 12,7.449 12,8 12,8.551 11.553,9 11,9 M11,5C9.699,5,8.599,5.839,8.185,7L2,7 2,9 3,9 3,11 4,11 4,9 5,9 5,11 6,11 6,9 8.185,9C8.599,10.161 9.699,11 11,11 12.654,11 14,9.654 14,8 14,6.346 12.654,5 11,5" />
                                                <GeometryDrawing Brush="{DynamicResource ControlBrushKey}"     Geometry="F1M12,8C12,7.449 11.553,7 11,7 10.449,7 10,7.449 10,8 10,8.551 10.449,9 11,9 11.553,9 12,8.551 12,8" />
                                              </DrawingGroup.Children>
                                            </DrawingGroup>
                                          </DrawingBrush.Drawing>
                                        </DrawingBrush>
                                      </Setter.Value>
