  <Style TargetType="{x:Type e:Asn1GridEntryContentPresenter}">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type e:Asn1GridEntryContentPresenter}">
          <ContentControl ContentTemplate="{TemplateBinding DisplayTemplate}"
                          Content="{Binding}"
                          Focusable="False"
                          Name="ContentPresenter"/>
          <ControlTemplate.Triggers>
            <Trigger Property="IsEditing" Value="True">
              <Setter TargetName="ContentPresenter" Property="ContentTemplate" Value="{Binding RelativeSource={RelativeSource TemplatedParent},Path=EditTemplate,Mode=OneWay}"/>
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
