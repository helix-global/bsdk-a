  <DataTemplate DataType="{x:Type e:ETraceInfo}">
    <u:TreeDataGrid ItemsSource="{Binding Entries}"
                    VirtualizingStackPanel.IsVirtualizing="True">
      <u:TreeDataGrid.Resources>
        <HierarchicalDataTemplate DataType="{x:Type e:ETraceInfoEntry}" ItemsSource="{Binding Entries}">
          <TextBlock Text="{Binding ShortName,Mode=OneWay}"/>
        </HierarchicalDataTemplate>
        <DataTemplate DataType="std:String">
          <TextBlock Text="{Binding}" Margin="5,0,5,0" TextTrimming="CharacterEllipsis"/>
        </DataTemplate>
      </u:TreeDataGrid.Resources>
      <u:TreeDataGrid.Columns>
        <u:TreeDataGridTextColumn Width="200" Header="Name"     Binding="{Binding ShortName}"/>
        <u:TreeDataGridTextColumn Width="200" Header="Duration" Binding="{Binding Duration}"/>
        <u:TreeDataGridTextColumn Width="300" Header="Duration" Binding="{Binding Duration}"/>
      </u:TreeDataGrid.Columns>
    </u:TreeDataGrid>
  </DataTemplate>
