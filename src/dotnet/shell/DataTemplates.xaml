        <TextBox BorderThickness="0" IsReadOnly="True" IsReadOnlyCaretVisible="True" Text="{Binding FriendlyNameFromLibraryRussian,Mode=OneWay}"/>
      </GroupBox>
    </Grid>
  </DataTemplate>
